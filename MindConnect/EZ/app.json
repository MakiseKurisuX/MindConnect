{
  "expo": {
    "name": "MindConnect",
    "slug": "MindConnect",
    "description": "MindConnect, or MC serves as an all-in-one mental-health companion, giving you reliable information, a supportive community, inspirational stories and professional counselling, in one application, which is easily accessible on your mobile devices.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/AppIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zacklow28.MindConnect"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/AppIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.zacklow28.MindConnect"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/AppIcon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "69d25956-b759-4ec8-9129-9ce2f263fbe0"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/69d25956-b759-4ec8-9129-9ce2f263fbe0"
    }
  }
}
